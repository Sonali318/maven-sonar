
pipeline
{
	agent any
		stages
			{
				stage('scm checkout')
					{
						steps
							{
								git branch: 'master', url: ' https://github.com/Sonali318/maven-project'
							}
					}
					stage('sonar and maven project')	
						{
							steps	
							{
							
								withSonarQubeEnv('newsonar') 
								{
									withMaven(jdk: 'localjdk-1.8', maven: 'localmaven') 
									{
									
									sh 'clean mvn install sonar:sonar'

								    }
								}
							
							}
									
						}
									
			}
									
}

										
